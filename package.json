{
  "name": "@stayradiated/flagship",
  "version": "1.0.0",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "dev": "remix dev",
    "start": "remix-serve build",
    "typecheck": "tsc",
    "tidy": "xo --fix ./app"
  },
  "dependencies": {
    "@chakra-ui/react": "2.8.0",
    "@emotion/cache": "11.11.0",
    "@emotion/react": "11.11.1",
    "@emotion/server": "11.11.0",
    "@emotion/styled": "11.11.0",
    "@remix-run/css-bundle": "1.19.1",
    "@remix-run/node": "1.19.1",
    "@remix-run/react": "1.19.1",
    "@remix-run/serve": "1.19.1",
    "date-fns": "2.30.0",
    "framer-motion": "10.13.1",
    "isbot": "3.6.13",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "xo": "0.55.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@remix-run/dev": "1.19.1",
    "@remix-run/eslint-config": "1.19.1",
    "@types/react": "18.2.17",
    "@types/react-dom": "18.2.7",
    "eslint": "8.45.0",
    "typescript": "5.1.6"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "xo": {
    "space": true,
    "semicolon": false,
    "prettier": true,
    "rules": {
      "n/file-extension-in-import": "off",
      "import/extensions": "Off",
      "@typescript-eslint/ban-types": "off",
      "unicorn/filename-case": "off"
    }
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": true,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "useTabs": false
  }
}
