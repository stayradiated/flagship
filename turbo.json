{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "tidy": {},
    "build": {
      "dependsOn": ["graphql", "^build"],
      "inputs": ["./src/**"],
      "outputs": ["./dist/**"]
    },
    "graphql": {
      "inputs": ["./src/**", "!./src/graphql.generated.ts"],
      "outputs": ["./src/graphql.generated.ts"]
    }
  }
}
